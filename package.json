{
  "name": "fastify-poc",
  "version": "1.0.0",
  "description": "Fastify POC with production ready features",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "test": "npm run test:tap && npm run test:jest",
    "test:tap": "tap test/**/*.test.js",
    "test:jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
